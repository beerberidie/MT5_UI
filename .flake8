[flake8]
max-line-length = 127
max-complexity = 10
exclude =
    .git,
    __pycache__,
    .venv,
    .venv311,
    venv,
    env,
    build,
    dist,
    *.egg-info,
    .tox,
    .pytest_cache,
    archive,
    tradecraft-console-main,
    ai_trading_system_modular_light_revision,
    INFO
ignore =
    E203,
    E501,
    W503,
    W504

